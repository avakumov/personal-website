{
  "_comment": "//TODO put it in order",
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "jsRules": {
    "max-line-length": {
      "options": [120]
    },
    "no-console": [true, "info", "log", "time", "timeEnd", "trace"]
  },

  "rules": {
    "max-line-length": {
      "options": [120]
    },
    "new-parens": true,
    "no-arg": true,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": false
  },
  "rulesDirectory": []
}
